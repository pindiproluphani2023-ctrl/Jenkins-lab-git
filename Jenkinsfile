    agent any
    stages {
        stage('Checkout') {
            steps {
                echo 'Checking out code from GitHub...'
            }
        }
        stage('Build') {
            steps {
                echo 'Building the project... Done by Pavan'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing the project... Passed'
            }
        }
    }
}
